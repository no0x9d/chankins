<h2><%= link @release.project.name, to: project_path(@conn, :show, @release.project) %></h2>

<h3><%= @release.name %></h3>
<h4>Versions:</h4>
<ul>
  <%= for version <- @release.versions do %>
    <li> <%= link version.version, to: version_path(@conn, :show, version) %> </li>
  <% end %>
</ul>

<span><%= link "Edit", to: release_path(@conn, :edit, @release) %></span>
<span><%= link "Releases", to: release_path(@conn, :index) %></span>
